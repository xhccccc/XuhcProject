<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xuhc.utils">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:requestLegacyExternalStorage="true">
        <activity android:name=".ShowActivity">
            <intent-filter>
                <action android:name="com.xuhc.xuhcproject.DATE_TIME_UTIL" />
                <action android:name="com.xuhc.xuhcproject.CATCH_CRASH" />
                <action android:name="com.xuhc.xuhcproject.NET_WORK_UTIL" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".UtilsActivity" />
    </application>

</manifest>