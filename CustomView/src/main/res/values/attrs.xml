<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SeekSettingView">
        <attr name="title" />
        <attr name="value" />
        <attr name="max_value" format="integer" />
        <attr name="min_value" format="integer" />
        <attr name="step" format="integer" />
        <attr name="left" format="reference"/>
        <attr name="right" format="reference"/>
    </declare-styleable>

    <!--自定义VoicePlayingIcon属性-->
    <declare-styleable name="VoicePlayingIcon">
        <attr name="pointer_color" format="color" />
        <attr name="pointer_num" format="integer" />
        <attr name="pointer_width" format="dimension" />
        <attr name="pointer_speed" format="integer" />
        <!--圆角的x半径-->
        <attr name="pointer_rx" format="dimension" />
        <!--圆角的y半径-->
        <attr name="pointer_ry" format="dimension" />
    </declare-styleable>

</resources>